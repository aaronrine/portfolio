asses=\"next\" onclick={setNextItem} label=\"&gt;\" />\n    </section>\n  );\n}\n\nexport default Carousel;\n","import ProjectCard from \"./ProjectCard\";\nimport { Carousel } from \"../Carousel/Carousel\";\nimport \"./ProjectCarousel.scss\";\n\nexport function ProjectCarousel({ projects }) {\n  return (\n      <Carousel \n        items={projects} \n        classes=\"ProjectCarousel\" \n        ItemCard={ProjectCard}\n        itemProps = {{\n          previous: {classes: \"prev\"},\n          current: {active: true},\n          next: {classes: \"next\"}\n        }}\n      />\n  );\n}\n\nexport default ProjectCarousel;\n","import ProjectCarousel from \"./ProjectCarousel\";\nexport { ProjectCarousel };\nexport default ProjectCarousel;\n","export function Footer() {\n  return <footer>Copyright &copy; 2020</footer>;\n}\n\nexport default Footer;\n","import { Link } from \"react-router-dom\";\nexport function Header() {\n  return (\n    <header className=\"Header\">\n      <div className=\"title\">\n        <Link to=\"/\" className=\"Link\">\n          <h1>Aaron Rine</h1>\n        </Link>\n        <h4>Helping to improve business web assets 1 step at a time.</h4>\n      </div>\n      <aside className=\"details\">\n        <a href=\"https://github.com/aaronrine\">Check out my Github</a>\n        <a href=\"https://www.linkedin.com/in/aaron-rine-1903b511a/\">\n          Follow me on LinkedIn to see my latest progress\n        </a>\n      </aside>\n    </header>\n  );\n}\n\nexport default Header;\n","export default __webpack_public_path__ + \"static/media/todo-app-phone-cropped.e4a298e5.png\";","export default __webpack_public_path__ + \"static/media/transparent-todo-app-phone-cropped.a94f1151.png\";","export default __webpack_public_path__ + \"static/media/transparent-propocket-checkout.275e9194.png\";","export default __webpack_public_path__ + \"static/media/transparent-propocket-desktop-shop.aa662995.png\";","export default __webpack_public_path__ + \"static/media/transpare